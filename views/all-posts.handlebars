<div class="posts">
    {{#each posts}}
    <article class="card">
        <div class="card-header">
            <h3>
                <a href="/post/{{id}}">{{title}}</a>
            </h3>
            <div>
                Post by {{User.username}}
            </div>
        </div>
        <div class="card-body">
            {{body}}
        </div>
    </article>
    {{/each}}
</div>